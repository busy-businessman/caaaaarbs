{
	"manifest_version": 2,
	"name": "Engram-to-Sheena",
	"version": "1.0",
	"description": "Replaces the word 'engram' with 'Sheena' for a more sensible browsing experience.",
	"icons": { "16": "images/sheena16.png",
           "48": "images/sheena48.png",
          "128": "images/sheena128.png" },
	"content_scripts": 
	[
		{
			"matches": ["*://*/*"], 
			"js": ["myscript.js"],
			"run_at": "document_end"  
		}
	]
}
